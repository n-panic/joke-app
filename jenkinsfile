pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh "echo 'Building...'"
                nodejs(configId: 'npm', nodeJSInstallationName: 'nodejs') {
                    sh 'npm install'
                    sh 'npm run build'
                }
                sh "echo 'Build complete'"
            }
            
        }
    }
}